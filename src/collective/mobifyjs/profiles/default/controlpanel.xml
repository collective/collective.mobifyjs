<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="collective.mobifyjs">

    <configlet
        title="Collective Mobify.js settings"
        action_id="collective.mobifyjs.settings"
        appId="collective.mobifyjs"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@collective.mobifyjs-settings"
        icon_expr="string:$portal_url/++resource++collective.mobifyjs/phone.png"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>